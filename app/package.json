{
  "name": "cms",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings.json",
    "test": "MOCHA_REPORTER=xunit TEST_CLIENT=0 meteor test --once --driver-package dispatch:mocha --port 3001",
    "test-graphical": "meteor test --driver-package practicalmeteor:mocha",
    "full-test": "meteor test --settings settings-dev.json --full-app --driver-package tmeasday:acceptance-test-driver",
    "chrome-test": "chimp .config/chimp.js --ddp=http://localhost:3000 --path=tests",
    "watch-api-test": "TEST_WATCH=1 meteor test --settings Settings-dev.json --driver-package dispatch:mocha",
    "watch-chimp-test": "chimp --ddp=http://localhost:3000 --watch --path=tests",
    "watch-firefox-test": "chimp --ddp=http://localhost:3000 --watch --path=tests --browser=firefox",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook"
  },
