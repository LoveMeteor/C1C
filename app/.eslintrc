{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true,
    "jquery": false,
    "meteor": true
    },
  "extends": ["airbnb","plugin:meteor/recommended"],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "meteor"
  ],
  "rules": {
    "semi" :"off",
    "no-underscore-dangle":["error",{ "allow": ["_id"] }],
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "import/extensions": ["off", "never"]
  }
}